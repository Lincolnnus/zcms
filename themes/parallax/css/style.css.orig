 * Lisence: MIT Lisence
 * Last-Modified: 2011/12/22
 */

/* 手動設定字體 */
/* 漢字手寫體（cursive） */
@font-face {
    font-family: 'ff-han';
    src: local(STFangsong), local(STKaiti), local(Kai), local("標楷體"), local(DFKaiShu-SB-Estd-BF);
    unicode-range: U+4E00-9FA5;
    font-weight: normal;
    font-style: normal;

}

@font-face {
    font-family: 'bf-han';
    src: local(STHeiti), local(SimHei), local(STXihei);
    unicode-range: U+4E00-9FA5;
    font-weight: normal;
    font-style: normal;

}

@font-face {
    font-family: 'BebasRegular';
    src: url('font/BEBAS___-webfont.eot');
    src: url('font/BEBAS___-webfont.eot?#iefix') format('embedded-opentype'),
         url('font/BEBAS___-webfont.woff') format('woff'),
         url('font/BEBAS___-webfont.ttf') format('truetype'),
         url('font/BEBAS___-webfont.svg#BebasRegular') format('svg');
    font-weight: normal;
    font-style: normal;

}
html,body{
 font-family:'BebasRegular';
	width:100%;
	height:100%;
}
#awrapper {
    width:100%;
    overflow:hidden;
}
#sidebar {
    float:left;
    width:22%;
    height:100%;
    position:fixed;
    z-index:100;}

#sidebar{
	background-image: linear-gradient(left bottom, rgb(200,229,255) 37%, rgb(224,240,255) 69%);
	background-image: -o-linear-gradient(left bottom, rgb(200,229,255) 37%, rgb(224,240,255) 69%);
	background-image: -moz-linear-gradient(left bottom, rgb(200,229,255) 37%, rgb(224,240,255) 69%);
	background-image: -webkit-linear-gradient(left bottom, rgb(200,229,255) 37%, rgb(224,240,255) 69%);
	background-image: -ms-linear-gradient(left bottom, rgb(200,229,255) 37%, rgb(224,240,255) 69%);

	background-image: -webkit-gradient(
		linear,
		left bottom,
		right top,
		color-stop(0.37, rgb(200,229,255)),
		color-stop(0.69, rgb(224,240,255)));
	box-shadow:inset 0px 10px 10px rgba(0,0,0,0.3);
}

.onemanlogo{
	position:relative;
	top:10px;
	left:15%;
	width:70%;
	margin-top:0px;
	margin-left:0px;
	z-index:1;
	opacity: 0.9;
}

.navigation{
	position:absolute;
	z-index:1;
	top:10px;
	width:100%;
}
.navigation li{
	font-family:'ff-han';/*could be bf*/
	color:#0066CC;
	display:block;
	padding: 0 10px;
	line-height:30px;
	margin-bottom:2px;
	font-weight:bold;
	-webkit-transition: all .5s ease－in－out;
	/*border-bottom:1px solid black;*/
	text-align:right;
	/*width:170px;*/
}
.navigation li:hover,.active{
	font-size:24pt;
	cursor:pointer;
	-webkit-transition: all .2s ease－in－out;
	width:100%!important;
}
